<p id="notice"><%= notice %></p>

<h1>Historials</h1>

<table>
  <thead>
    <tr>
      <th>Padecimiento</th>
      <th>Status</th>
      <th>Patologico</th>
      <th>Familiares</th>
      <th>Examen</th>
      <th>Anexo</th>
      <th>Unificada</th>
      <th>Autorizar</th>
      <th>Firma</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historials.each do |historial| %>
      <tr>
        <td><%= historial.padecimiento %></td>
        <td><%= historial.status %></td>
        <td><%= historial.patologico %></td>
        <td><%= historial.familiares %></td>
        <td><%= historial.examen %></td>
        <td><%= historial.anexo %></td>
        <td><%= historial.unificada %></td>
        <td><%= historial.autorizar %></td>
        <td><%= historial.firma %></td>
        <td><%= historial.customer %></td>
        <td><%= link_to 'Show', historial %></td>
        <td><%= link_to 'Edit', edit_historial_path(historial) %></td>
        <td><%= link_to 'Destroy', historial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Historial', new_historial_path %>
